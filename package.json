    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-vue": "^9.31.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
