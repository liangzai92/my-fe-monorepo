    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css,scss,vue,svelte}\"",
    "clean": "turbo clean && rm -rf node_modules .turbo",
    "test": "turbo test"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.20.0",
